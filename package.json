{
  "name": "lib-global",
  "version": "2.2.2",
  "description": "Lib for expanding functionality and deduplicating code between services",
  "main": "compiled/index.js",
  "scripts": {
    "clean": "rm -rf compiled",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --fix --ext .js src",
    "test": "mocha test",
    "build": "babel src -d compiled",
    "release": "yarn lint && yarn clean && yarn build && yarn test",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "postversion": "git push && git push --tags"
  },
  "homepage": "https://github.com/brandsExclusive/lib-global#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandsExclusive/lib-global.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.0.0",
    "mocha": "^5.2.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.3"
  }
}
